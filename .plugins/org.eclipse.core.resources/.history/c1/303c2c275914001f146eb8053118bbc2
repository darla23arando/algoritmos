package desafio33;

import java.util.Scanner;

public class desafio33 {
	public static void main(String[] args) throws InterruptedException {
		Scanner entrada = new Scanner(System.in);
		
		for (int i = 0; i <= 11; i++) {
			System.out.println("Jugadores: " + i);
			Thread.sleep(200);
			System.out.println("Edad:");
		}
		
		
}	
}
